<?xml version='1.0' encoding='utf-8'?>
<widget id="com.grownodes.growapp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Grow Nodes</name>
    <description>
        Manage Grow Nodes.
    </description>
    <author email="app@grownodes.com" href="https://grownodes.com">
        Grow Nodes
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-fcm" spec="1.1.5" />
    <plugin name="cordova-plugin-fcm-config-grownodes" source="pgb" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="com.example.hello" spec="https://github.com/don/cordova-plugin-hello.git" />
</widget>
